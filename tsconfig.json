{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "jsx":"react-native",
    "types": ["jest"],
    "baseUrl": ".",
    "paths": {
      "screens/*":["screens/*"],
      "components/*":["components/*"],
      "types/*":["types/*"],
      "hooks/*":["hooks/*"],
      "api/*":["api/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["**/node_modules", "**/Pods"],
  "allowSyntheticDefaultImports": true
}
